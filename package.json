{
  "name": "realenhance-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "install-all": "pnpm install -w",
    "build": "pnpm --filter ./client... run build && pnpm --filter @realenhance/server run build",
    "start": "pnpm --filter @realenhance/server run start"
  },
  "pnpm": {
    "overrides": {
      "@types/express": "^4.17.21",
      "@types/express-serve-static-core": "^4.17.41",
      "@types/body-parser": "1.19.5",
      "@types/serve-static": "1.15.5",
      "@types/express-session": "1.17.7",
      "@types/passport": "^1.0.17",
      "@types/passport-google-oauth20": "2.0.11",
      "@types/passport-oauth2": "^1.8.0"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6"
  }
}
