{
  "name": "realenhance-monorepo",
  "private": true,
  "workspaces": ["client", "server", "worker"],
  "scripts": {
    "install-all": "pnpm install --no-frozen-lockfile",
    "build": "pnpm --filter ./client... run build && pnpm --filter @realenhance/server run build",
    "start": "pnpm --filter @realenhance/server run start"
  },
  "pnpm": {
    "overrides": {
      "@types/express": "4.17.25",
      "@types/express-serve-static-core": "4.17.41",
      "@types/body-parser": "1.19.5",
      "@types/serve-static": "1.15.5",

      "@types/express-session": "1.17.7",
      "@types/passport": "1.0.16",
      "@types/passport-google-oauth20": "2.0.11",
      "@types/passport-oauth2": "1.6.11"
    }
  }
}



